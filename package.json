{
  "name": "minhas-despesas-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Jo√£o Pedro <joaopedromoraesnsilva@gmail.com>",
  "repository": "https://github.com/JPedro109/minhas-despesas-monolithic.git",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky",
    "migrate:generate": "prisma migrate dev && prisma generate",
    "migrate:execute": "prisma generate && prisma migrate deploy",
    "dev": "yarn migrate:execute && tsx watch ./src/server.ts",
    "build": "tsc -p tsconfig-build.json && tsc-alias -p tsconfig-build.json",
    "start": "yarn migrate:execute && yarn build && node dist/server.js",
    "test": "jest --passWithNoTests --detectOpenHandles --silent",
    "test:coverage": "yarn test --coverage",
    "test:staged": "yarn test --findRelatedTests",
    "test:watch": "yarn test --watchAll",
    "test:unit": "yarn test -c jest-unit-config.js",
    "test:integration": "yarn test -c jest-integration-config.js",
    "test:unit:watch": "yarn test --watchAll -c jest-unit-config.js",
    "test:integration:watch": "yarn test --watchAll -c jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles",
    "test:watch:verbose": "yarn test:verbose --watchAll",
    "test:unit:verbose": "yarn test:verbose -c jest-unit-config.js",
    "test:integration:verbose": "yarn test:verbose -c jest-integration-config.js",
    "test:unit:watch:verbose": "yarn test:verbose --watchAll -c jest-unit-config.js",
    "test:integration:watch:verbose": "yarn test:verbose --watchAll -c jest-integration-config.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@prisma/client": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "juice": "^11.0.0",
    "nodemailer": "^6.10.0",
    "pdf-lib": "^1.17.1",
    "prisma": "^6.0.1",
    "sharp": "^0.33.5",
    "stripe": "^17.4.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  }
}
